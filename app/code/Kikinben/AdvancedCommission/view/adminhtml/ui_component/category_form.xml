<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="content">
       
     <field name="kikinben_fulfilled">
         <argument name="data" xsi:type="array">
         <item name="options" xsi:type="object">Magento\Config\Model\Config\Source\Yesno</item>
             <item name="config" xsi:type="array">
                 <item name="sortOrder" xsi:type="number">60</item>
                 <item name="dataType" xsi:type="string">string</item>
                 <item name="formElement" xsi:type="string">select</item>
                 <item name="label" xsi:type="string" translate="true">Fullfiled by Kikinben</item>
             </item>
         </argument>
     </field>    
	
	<field name="kikinben_percentage_amount">
         <argument name="data" xsi:type="array">
         <item name="options" xsi:type="object">Magento\Config\Model\Config\Source\Yesno</item>
             <item name="config" xsi:type="array">
                 <item name="sortOrder" xsi:type="number">61</item>
                 <item name="dataType" xsi:type="string">string</item>
                 <item name="formElement" xsi:type="string">select</item>
                 <item name="label" xsi:type="string" translate="true">Kikinben Commission Type as Percentage</item>
             </item>
         </argument>
     </field>  
                
        
        <field name="kikinben_product_commission">
            <argument name="data" xsi:type="array">                          
                <item name="config" xsi:type="array">                    
                    <item name="formElement" xsi:type="string">input</item>
	            	<item name="label" xsi:type="string" translate="true">Kikinben Product Advanced Commission</item>
	            	<item name="dataType" xsi:type="string">string</item>                                                            
                    <item name="sortOrder" xsi:type="number">62</item>                   
                </item>
            </argument>
        </field>
    </fieldset>
</form>